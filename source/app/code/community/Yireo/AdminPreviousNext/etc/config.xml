<config>

    <modules>
        <Yireo_AdminPreviousNext>
            <version>0.0.3</version>
        </Yireo_AdminPreviousNext>
    </modules>

    <global>
        <blocks>
            <adminpreviousnext>
                <class>Yireo_AdminPreviousNext_Block</class>
            </adminpreviousnext>
        </blocks>

        <helpers>
            <adminpreviousnext>
                <class>Yireo_AdminPreviousNext_Helper</class>
            </adminpreviousnext>
        </helpers>

        <models>
            <adminpreviousnext>
                <class>Yireo_AdminPreviousNext_Model</class>
            </adminpreviousnext>
        </models>
	</global>

    <adminhtml>
        <layout>
            <updates>
                <adminpreviousnext>
                    <file>adminpreviousnext.xml</file>
                </adminpreviousnext>
            </updates>
        </layout>
        <translate>
            <modules>
                <Yireo_AdminPreviousNext>
                    <files>
                        <default>Yireo_AdminPreviousNext.csv</default>
                    </files>
                </Yireo_AdminPreviousNext>
            </modules>
        </translate>
    </adminhtml>
</config>
